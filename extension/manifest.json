{
  "manifest_version": 3,
  "name": "EarthBuddy",
  "version": "1.0.0",
  "description": "Sustainable products reporting solution",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "tabs",
    "webNavigation",
    "alarms"
  ],
  "host_permissions": [
    "https://phishtank.vercel.app/api/*",
    "https://urlscan.io/*",
    "http://*/*",
    "https://*/*"
  ]
}